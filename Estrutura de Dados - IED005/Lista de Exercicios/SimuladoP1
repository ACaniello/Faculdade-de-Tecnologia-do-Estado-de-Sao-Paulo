4 - Qual dos métodos abaixo está correto -

public class Lista {
  long tamanho;
  No inicio;
  No fim;
  
  Lista(){
    tamanho = 0;
    inicio = null;
    fim = null;
  }

-------------------------------A-------------------------------

void adicionar (Object info) {
  No no = new No(info);
  if (estaVazia()) {
    inicio = no;
  }
  fim = no;
  tamanho++;
}

-------------------------------B-------------------------------

void adicionar(Object info);
  No no = new No(info);
  if(estaVazia()) {
    inicio = no;
  }
  else {
    fim.prox = no;
  }
  
  fim = no;
  tamanho++;
}
-------------------------------C-------------------------------

void adicionar(Object info) {
  No no = new No(info);
  fim = no;
  if (estaVazia()) {
    inicio = no;
  }
  else {
    fim.prox = no;
  }
  tamanho++;
}

